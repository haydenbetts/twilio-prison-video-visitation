<?xml version="1.0" encoding="utf-8"?>
<level-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:maxLevel="0">
        <shape android:shape="rectangle">
            <solid android:color="#fff"/>
        </shape>
    </item>
    <item android:maxLevel="1">
        <shape android:shape="rectangle">
            <solid android:color="#97c93d"/>
            <corners android:radius="2dp"/>
        </shape>
    </item>
    <item android:maxLevel="2">
        <shape android:shape="rectangle">
            <solid android:color="#c9983c"/>
            <corners android:radius="2dp"/>
        </shape>
    </item>
    <item android:maxLevel="3">
        <shape android:shape="rectangle">
            <solid android:color="#cccccc"/>
            <corners android:radius="2dp"/>
        </shape>
    </item>
</level-list>
