    <TextView android:id="@+id/labelLastName" android:layout_marginTop="@dimen/enlarged_offset" android:text="@string/label_last_name" android:labelFor="@+id/editInmateLastName" app:layout_constraintStart_toStartOf="@+id/labelState" app:layout_constraintTop_toBottomOf="@+id/editInmateFirstName" style="@style/TextView.Label"/>
    <EditText android:id="@+id/editInmateLastName" app:layout_constraintEnd_toEndOf="0" app:layout_constraintStart_toStartOf="@+id/labelLastName" app:layout_constraintTop_toBottomOf="@+id/labelLastName" style="@style/EditText.Register"/>
    <TextView android:id="@+id/labelMiddleName" android:layout_marginTop="@dimen/enlarged_offset" android:text="@string/label_middle_name" android:labelFor="@+id/editInmateMiddleName" app:layout_constraintStart_toStartOf="@+id/labelState" app:layout_constraintTop_toBottomOf="@+id/editInmateLastName" style="@style/TextView.Label"/>
    <EditText android:id="@+id/editInmateMiddleName" app:layout_constraintEnd_toEndOf="0" app:layout_constraintStart_toStartOf="@+id/labelMiddleName" app:layout_constraintTop_toBottomOf="@+id/labelMiddleName" style="@style/EditText.Register"/>
    <TextView android:id="@+id/labelSuffix" android:layout_marginTop="@dimen/enlarged_offset" android:text="@string/label_suffix" android:labelFor="@+id/editSuffix" app:layout_constraintStart_toStartOf="@+id/labelState" app:layout_constraintTop_toBottomOf="@+id/editInmateMiddleName" style="@style/TextView.Label"/>
    <EditText android:id="@+id/editSuffix" app:layout_constraintEnd_toEndOf="0" app:layout_constraintStart_toStartOf="@+id/labelSuffix" app:layout_constraintTop_toBottomOf="@+id/labelSuffix" style="@style/EditText.Register"/>
    <TextView android:id="@+id/labelDateOfBirth" android:layout_marginTop="@dimen/enlarged_offset" android:text="@string/label_date_of_birth" android:labelFor="@+id/editDateOfBirth" app:layout_constraintStart_toStartOf="@+id/labelState" app:layout_constraintTop_toBottomOf="@+id/editSuffix" style="@style/TextView.Label"/>
    <TextView android:gravity="left|center_vertical|center_horizontal|center|start" android:id="@+id/editDateOfBirth" app:layout_constraintEnd_toEndOf="0" app:layout_constraintStart_toStartOf="@string/label_date_of_birth" app:layout_constraintTop_toBottomOf="@+id/labelDateOfBirth" style="@style/EditText.Register.DateOfBirth"/>
    <androidx.constraintlayout.widget.Group android:id="@+id/groupAddInmate" android:visibility="gone" android:layout_width="match_parent" android:layout_height="match_parent" app:constraint_referenced_ids="labelDateOfBirth,editDateOfBirth,labelSuffix,editSuffix,labelMiddleName,editInmateMiddleName,labelLastName,editInmateLastName,labelFirstName,editInmateFirstName,labelInmateId,editInmateId"/>
