<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:focusableInTouchMode="true" android:descendantFocusability="beforeDescendants" android:layout_width="match_parent" android:layout_height="wrap_content">
    <TextView android:id="@+id/labelState" android:layout_marginTop="@dimen/base_offset" android:text="@string/label_state_incarceration" android:layout_marginStart="@dimen/base_offset" app:layout_constraintStart_toStartOf="0" app:layout_constraintTop_toTopOf="0" style="@style/TextView.Label"/>
    <Spinner android:id="@+id/spinnerState" app:layout_constraintEnd_toEndOf="0" app:layout_constraintStart_toStartOf="@+id/labelState" app:layout_constraintTop_toBottomOf="@+id/labelState" style="@style/Spinner.Register"/>
    <TextView android:id="@+id/labelFaculty" android:layout_marginTop="@dimen/enlarged_offset" android:text="@string/label_facility" app:layout_constraintStart_toStartOf="@+id/labelState" app:layout_constraintTop_toBottomOf="@+id/spinnerState" style="@style/TextView.Label"/>
    <Spinner android:id="@+id/spinnerFacility" app:layout_constraintEnd_toEndOf="0" app:layout_constraintStart_toStartOf="@+id/labelState" app:layout_constraintTop_toBottomOf="@+id/labelFaculty" style="@style/Spinner.Register"/>
    <TextView android:id="@+id/labelInmateName" android:layout_marginTop="@dimen/enlarged_offset" android:text="@string/label_inmate_name" android:labelFor="@+id/editInmateName" app:layout_constraintStart_toStartOf="@+id/labelState" app:layout_constraintTop_toBottomOf="@+id/spinnerFacility" style="@style/TextView.Label"/>
    <com.forasoft.homewavvisitor.ui.views.InstantAutoCompleteTextView android:textSize="@dimen/standard" android:textColorHint="@color/textGray" android:id="@+id/editInmateName" android:hint="@string/placeholder_select_inmate" android:completionThreshold="0" app:layout_constraintEnd_toEndOf="0" app:layout_constraintStart_toStartOf="@+id/labelInmateName" app:layout_constraintTop_toBottomOf="@+id/labelInmateName" style="@style/EditText.Register.Autocomplete"/>
    <TextView android:id="@+id/labelInmateId" android:layout_marginTop="@dimen/enlarged_offset" android:text="@string/label_inmate_id" android:labelFor="@+id/editInmateId" app:layout_constraintStart_toStartOf="@+id/labelState" app:layout_constraintTop_toBottomOf="@+id/editInmateName" style="@style/TextView.Label"/>
    <EditText android:id="@+id/editInmateId" android:digits="0123456789" app:layout_constraintEnd_toEndOf="0" app:layout_constraintStart_toStartOf="@+id/labelState" app:layout_constraintTop_toBottomOf="@+id/labelInmateId" style="@style/EditText.Register.Numeric"/>
    <TextView android:id="@+id/labelFirstName" android:layout_marginTop="@dimen/enlarged_offset" android:text="@string/label_first_name" android:labelFor="@+id/editInmateFirstName" app:layout_constraintStart_toStartOf="@+id/labelState" app:layout_constraintTop_toBottomOf="@+id/editInmateId" style="@style/TextView.Label"/>
