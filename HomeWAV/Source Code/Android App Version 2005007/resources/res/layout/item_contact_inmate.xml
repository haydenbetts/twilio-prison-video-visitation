    <Spinner android:enabled="false" android:id="@+id/spinnerRelationship" app:layout_constraintEnd_toEndOf="0" app:layout_constraintStart_toStartOf="@+id/labelRelationship" app:layout_constraintTop_toBottomOf="@+id/labelRelationship" style="@style/Spinner.Register"/>
